 create secret generic flask-app-secrets   
 --from-literal=DB_HOST=db   
 --from-literal=DB_USER=gif_user   
 --from-literal=DB_PASSWORD=gif_password   
 --from-literal=DB_NAME=gif_db   
 --from-literal=MYSQL_ROOT_PASSWORD=rootpassword   
 --from-literal=MYSQL_DATABASE=gif_db   
 --from-literal=MYSQL_USER=gif_user   
 --from-literal=MYSQL_PASSWORD=gif_password
